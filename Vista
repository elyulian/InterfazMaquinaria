    private void procesarActionPerformed(java.awt.event.ActionEvent evt) {                                         
   int a=0, b=0, contador=0;
   double c=0.0;
        switch(t1.getText()){
        
        case "grua":
            if(Double.parseDouble(ta2.getText())== 5 || contador ==5){
               
             JOptionPane.showMessageDialog(null, "CANTIDAD DE MAQUINAS AGOTADAS","Error ingreso",JOptionPane.ERROR_MESSAGE);
           t1.setText("");
            t2.setText("");
            t4.setText("");
            
             break;
            }
            t4.setText("10000000");
            t5.setText(m1.getDescripcion());
            
          a = m1.getCantidad();
          
          b= Integer.parseInt(t2.getText());
          contador+=b;
          if(b<0){
           
          JOptionPane.showMessageDialog(null, "INGRESE UN NUMERO VALIDO","Error ingreso",JOptionPane.ERROR_MESSAGE);
          t1.setText("");
            t2.setText("");
            t4.setText("");
          break;
          }
          if(b>0 && b<=5){
            ta1.setText(String.valueOf(a-b));
            ta2.setText(t2.getText());
          }else{
              JOptionPane.showMessageDialog(null, "SOLO HAY 5 MAQUINAS DISPONIBLE DE ESTA","Error ingreso",JOptionPane.ERROR_MESSAGE);
          }
          c = m1.getCosto()*Double.parseDouble(ta2.getText());
            m1.setContadorMD(c);
            t6.setText(String.valueOf(c));
         
            break;
